<header>
  <h1>Tickets</h1>
</header>

<div class="table-responsive">
  <table class="table table-striped table-hover tickets">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Comments</th>
        <th scope="col">State</th>
      </tr>
    </thead>
    <tbody>
      <%= render @project.tickets %>
    </tbody>
  </table>
</div>
